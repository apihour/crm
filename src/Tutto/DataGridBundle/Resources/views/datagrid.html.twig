{% use 'TuttoXhtmlBundle::xhtml.html.twig' %}{% use 'TuttoCommonBundle::flash-bag.html.twig' %}{% use 'TuttoDataGridBundle::paginator.html.twig' %}{% use '::flash-bag.html.twig' %}{% block grid_search_form %}    <form method="post" action="{{ grid.generateRoute({page: 1, order: 'desc', sort: ''}) }}">    </form>{% endblock grid_search_form %}{% block grid_content %}    <script>        $(function() {            $('table').delegate('input[type="checkbox"].selectall', 'change', function() {                alert('asd');            });        });    </script>    {{ block('flash_bag') }}    {% set attribs = grid.attribs() %}    <table {{ block('grid_attributes') }}>        {%- block grid_table_head -%}        <thead>        <tr>            {% for col in grid.columns() %}                {% set label, attribs = col.getLabel(), col.getAttributes() %}                <th {{ block('grid_attributes') }}>{{ block('grid_head_label') }}</th>            {% endfor %}        </tr>        </thead>        {%- endblock grid_table_head -%}        {%- block grid_table_body -%}        <tbody>        {% for row in grid.results() %}            <tr>            {% for col in row.getEvents() %}                {% set tag = col.decorate() %}                {% set attribs = col.getColumn().getAttributes() %}                <td {{ block('grid_attributes') }}>                    {% if col.getColumn().isTranslate() %}                    {{ block('xhtml')|trim|trans }}                    {% else %}                        {{ block('xhtml') }}                    {% endif %}                </td>            {% endfor %}            </tr>        {% endfor %}        </tbody>        {%- endblock grid_table_body -%}        {%- block grid_table_footer -%}        {%- endblock grid_table_footer -%}    </table>    <div class="row">        <div class="col-md-6">            <div class="dataTables_info">                {{ "paginator.show_results_info"|trans({"%from%": 1, "%to%": 2, "%all%": grid.paginator.count()}) }}            </div>        </div>        <div class="col-md-6"><div class="dataTables_paginate">{{ block('grid_paginator') }}</div></div>    </div>{% endblock grid_content %}{%- block grid_head_label -%}{% spaceless %}    {% if label is string %}        {% if label is not empty %}{{ label|trans|trim|raw }}{% endif %}    {% elseif label is object %}        {% set tag = label %}        {{ block('xhtml') }}    {% endif %}    {% endspaceless %}{%- endblock grid_head_label -%}{%- block grid_attributes -%}{% spaceless %}    {% set attributes = ' ' %}    {% for name, values in attribs %}        {% set attributes = attributes~name~'="'~values|join(" ")~'" ' %}    {% endfor %}{{ attributes|raw }}    {% endspaceless %}{%- endblock grid_attributes -%}